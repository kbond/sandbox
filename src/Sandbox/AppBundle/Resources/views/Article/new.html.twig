{% extends '::base.html.twig' %}

{% block title %}New Article{% endblock %}

{% block body %}
    <form class="form-horizontal" method="post" novalidate="novalidate" {{ form_enctype(form) }}>
        {{ form_widget(form) }}

        <div class="form-actions">
            <button class="btn btn-primary" type="submit">Create</button>
            <a class="btn" href="{{ path('article') }}">Cancel</a>
        </div>
    </form>
{% endblock %}