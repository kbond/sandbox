<a title="Delete" class="btn btn-mini btn-danger method-delete" href="{{ path('zenstruck_media_delete_'~(file.isDir ? 'dir' : 'file'), manager.requestParams({ 'filename': file.getFilename })) }}"><i class="icon-trash icon-white"></i></a>
<a title="Rename" href="#" class="btn btn-mini zenstruck-media-rename" data-name="{{ file.getBasename('.'~file.getExtension) }}" data-url="{{ path('zenstruck_media_rename_'~(file.isDir ? 'dir' : 'file'), manager.requestParams({ 'filename': file.getFilename })) }}"><i class="icon-edit"></i></a>